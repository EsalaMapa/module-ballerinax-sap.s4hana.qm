            "schema": {
              "type": "string"
            }
          },
          {
            "$ref": "#/components/parameters/count"
          },
          {
            "$ref": "#/components/parameters/A_CharcAttribSeldCodeSetOrderByOptions"
          },
          {
            "$ref": "#/components/parameters/A_CharcAttribSeldCodeSetSelectOptions"
          }
        ],
        "responses": {
          "200": {
            "description": "Retrieved entities",
            "content": {
